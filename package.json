    "@types/axios": "^0.9.36",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/sequelize": "^6.12.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
